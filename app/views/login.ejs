<!DOCTYPE html>
<html xmlns:ng="http://angularjs.org">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">

    <title>SMUX</title>

    <link href="css/bootstrap.css" rel="stylesheet">
    <link href="css/custom.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="css/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="http://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href="http://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic" rel="stylesheet" type="text/css">
    <style>
    body{
        background-image: url("images/smu.JPG");
        background-repeat: no-repeat;
        background-size: cover;
    }

    .container{
        background-color: white;
        opacity: 0.9;
    }

    hr{
        border-width: 2px;
    }
    .form-signin-heading{
    }
    </style>
</head>
<body ng-app="smux">
<div class= "col-md-12">
    <img src="images/smu_logo.png" style="max-height: 20%"/>

</div>
<!--
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display->
            <div class="navbar-header page-scroll">
                <a class="navbar-brand" href="/">SMUX</a>
            </div>
        </div>
    </nav>
-->

    <!-- Portfolio Grid Section -->
    <section id="portfolio">
        <div class="container col-md-offset-4 col-md-4 col-sm-12">
            <form class="form-signin" action="/login" method="post">
                <h2 class="form-signin-heading text-center">Open Sesame!</h2>
                
            <hr>
                <label for="username" class="sr-only">Username</label>
                <input type="text" name="username" id="username" class="form-control" placeholder="Username" required autofocus />
                <br/>
                <label for="password" class="sr-only">Password</label>
                <input type="password" name="password" id="password" class="form-control" placeholder="Password" required autofocus />
                <br/>
                <button class="btn btn-lg btn-primary btn-block" type="submit">Login</button>
            </form>

            <br>

            
        </div>
    </section>
    
    <script type="text/javascript" src="/js/jquery.js"></script>
    <script type="text/javascript" src="/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="/js/custom.js"></script>

    <script type="text/javascript" src="/js/angular/angular.js"></script>
    <script type="text/javascript" src="/js/angular-route/angular-route.min.js"></script>
    <script type="text/javascript" src="/js/angular-resource/angular-resource.min.js"></script>
    
